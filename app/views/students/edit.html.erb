<% if flash[:errors] %>
  <% flash[:errors].each do |e| %>
    <%= e %>
  <% end %>
<%end%>


<%=form_with model: @student do |f|%>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :major %>
  <%= f.text_field :major %>

  <%= f.label :age %>
  <%= f.text_field :age %>

  <%= f.collection_select :instructor_id, Instructor.all, :id, :name %>

  <%= f.submit %>
<% end %>